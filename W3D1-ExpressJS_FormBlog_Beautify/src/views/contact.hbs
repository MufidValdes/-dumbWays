<!DOCTYPE html>
<html lang="en">
{{!-- ======== Head ======== --}}
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Add Project</title>
  <!-- Bootstrap CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
  <link rel="stylesheet" href="https://kit.fontawesome.com/917179b360.css" crossorigin="anonymous">
</head>
{{!-- ======== Body ======== --}}
<body>
   <!-- Header -->
  <header id="header">
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
      <div class="container">
        <a class="navbar-brand" href="/">
          <img src="https://dumbways.id/assets/images/brandred.png" alt="logo" width="40" >
        </a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNavDropdown">
          <ul class="navbar-nav w-100 mb-2 mt-2 mb-lg-0">
            <li class="nav-item">
              <a class="nav-link" href="./myproject">Add Project</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="./testimonial">My Testimonial</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="./contact">Contact Me</a>
            </li>
            <li class="nav-item ms-auto">
              <button type="button" class="btn btn-success">Login</button>
            </li>
          </ul>
        </div>
        
      </div>
    </nav>
  </header>

  <!-- Main -->
  <main class="container mt-5">
    <div class="Contact">
      <form class="mx-auto" style="max-width: 500px;">
        <h1 class="text-center mb-4">GET IN TOUCH</h1>
        <div class="mb-3">
          <label for="inputName" class="form-label">Name</label>
          <input id="inputName" type="text" class="form-control" required placeholder="Input your Name">
        </div>
        <div class="mb-3">
          <label for="inputEmail" class="form-label">Email</label>
          <input id="inputEmail" type="email" class="form-control" required placeholder="Input your Email">
        </div>
        <div class="mb-3">
          <label for="inputNumber" class="form-label">Phone Number</label>
          <input id="inputNumber" type="tel" class="form-control" required placeholder="Input your Number Phone">
        </div>
        <div class="mb-3">
          <label for="subject" class="form-label">Subject</label>
          <select id="subject" class="form-select">
            <option hidden>Select Subject!</option>
            <option>Question</option>
            <option>Hire</option>
            <option>Let's Connect</option>
          </select>
        </div>
        <div class="mb-3">
          <label for="message" class="form-label">Message</label>
          <textarea id="message" class="form-control" rows="4"></textarea>
        </div>
        <div class="d-grid">
          <button type="submit" class="btn btn-success">Submit</button>
        </div>
      </form>
    </div>
  </main>

  <!-- Footer -->
  <footer></footer>

 <!-- Bootstrap JS -->
  <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js" integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.min.js" integrity="sha384-cVKIPhGWiC2Al4u+LWgxfKTRIcfu0JTxR+EQDz/bgldoEyl4H0zUF0QKbrJ0EcQF" crossorigin="anonymous"></script>
  <script src="https://kit.fontawesome.com/917179b360.js" crossorigin="anonymous"></script>
</body>

</html>
